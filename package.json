{
  "name": "fb-search",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "web": "vite",
    "web:build": "vite build",
    "build": "bun build src/index.ts --target bun --outdir ./dist",
    "start": "NODE_ENV=production bun dist/index.js",
    "test": "bun test",
    "parse": "bun run ./scripts/parse-inpx",
    "schema": "bun run schema:generate && bun run schema:migrate",
    "schema:generate": "drizzle-kit generate --dialect postgresql --schema ./src/core/db/schema.ts",
    "schema:migrate": "bun run ./scripts/migrate.ts"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@tsconfig/svelte": "5.0.5",
    "@types/bun": "latest",
    "@types/node": "latest",
    "@types/pg": "8.15.5",
    "@typescript-eslint/eslint-plugin": "8.9.0",
    "@typescript-eslint/parser": "8.9.0",
    "@unocss/reset": "66.5.1",
    "drizzle-kit": "0.31.4",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-svelte": "2.45.1",
    "prettier": "3.3.3",
    "prettier-plugin-svelte": "3.4.0",
    "svelte": "4.2.19",
    "svelte-check": "4.0.5",
    "tslib": "2.8.1",
    "typescript": "5.9.2",
    "unocss": "66.5.1",
    "vi-fetch": "0.8.0",
    "vite": "5.4.9"
  },
  "dependencies": {
    "@elysiajs/static": "1.3.0",
    "@elysiajs/swagger": "1.3.1",
    "csv-parse": "5.5.6",
    "drizzle-orm": "0.44.5",
    "elysia": "1.3.21",
    "node-stream-zip": "1.15.0",
    "pg": "8.16.3",
    "transliteration": "2.3.5",
    "wretch": "2.11.0"
  }
}
